  "name": "decision-making-tool",
  "version": "1.0.0",
  "main": "",
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/format": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "bezier-easing": "^2.1.0",
    "conventional-changelog-atom": "^5.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unicorn": "^57.0.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "stylelint": "^16.15.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "npx eslint --fix .",
    "prepare": "cd .. && husky decision-making-tool/.husky",
    "format": "prettier --write \"src/**/*\" --ignore-unknown",
    "format:check": "prettier --check \"src/**/*\" --ignore-unknown",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx,json,scss,css,md}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "bezier-js": "^6.1.4"
  }
}
